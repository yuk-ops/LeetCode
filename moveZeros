class Solution {
    public void moveZeroes(int[] nums) {
        int x=0;
        int h=0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]!=0)
            {
                x=nums[i];
                nums[i]=nums[h];
                nums[h]=x;
                h++;
            }
        }
            
    }
}
